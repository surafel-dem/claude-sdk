{
  "mcpServers": {
    "firecrawl-mcp": {
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": "<FIRECRAWL_API_KEY>"
      }
    },
    "e2b-sandbox": {
      "command": "uv",
      "args": ["--directory", "../sandbox_mcp", "run", "server.py"],
      "env": {
        "E2B_API_KEY": "<E2B_API_KEY>"
      }
    }
  }
}
